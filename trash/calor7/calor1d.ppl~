 -module {}\ 
 -unit_src_file {}\ 
 -priority 100\ 
 -algebra_in {}\ 
 -algebra_out {set_by_comm set_by_comm set_by_comm set_by_comm set_by_comm}\ 
 -algebra_times_tags {}\ 
 -expand 1\ 
 -algebra_class user\ 
 -process_grid {1 1}\ 
 -algebra_distributors {}\ 
 -unit_ondriver LAUNCHED-BY-PALM\ 
 -unit_filename {}\ 
 -unit_type UNIT\ 
 -working_directory .\ 
 
